{#
/**
 * @file
 * Theme override to display a formatted responsive image field.
 *
 * Available variables:
 * - responsive_image: A collection of responsive image data.
 * - url: An optional URL the image can be linked to.
 *
 * @see template_preprocess_responsive_image_formatter()
 *
 * This will be fixed in https://www.drupal.org/project/drupal/issues/3064751. Once that commit is fixed, this template
 * can be removed.
 */
#}
{% if url %}
  {{ link(responsive_image, url) }}
{% else %}
  {{ responsive_image }}
{% endif %}
